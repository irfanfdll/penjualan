switch ($supir) {
        case "Manusia":
            $a = $this->beli_komputer();
            break;
        case "Malaikat":
            $a = $this->beli_laptop();

            break;
        case "Jin":
            $a = $this->beli_chromebook();

            break;
        case "Setan":
            $a = $this->etan();

            break;
            return "$a <br />;
    }